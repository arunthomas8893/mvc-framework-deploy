{%  extends "base.twig" %}

{% block body %}
	<!----Header Section End-->
	<!---------------------Page Container Section---------------->
	<!---Page Navigation-->
		<div id="pageNav" class="section"> <div class="innerSection">
			<a href="/">Home</a><img src="/image/icon/Right arrow.png" alt="">
			<a href="">Support</a>
		</div>
	</div>
	<section id="supportContainer" class="section">
		<div class="innerSection">
			<div class="contentDiv">
				<h3 class="title">Support</h3>
				<ul>
					<li>WhatsApp : +91 98765 43215
					</li>
					<li>Customer Care : +91 98765 43215, 0484 5623458
					</li>
				</ul>
			</div>
			<div class="contentDiv">
